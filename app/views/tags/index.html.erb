<div id="container">
  <%= image_tag "background.jpg", id: "background" %>
  <% @tags.each do |tag| %>
    <% name = Character.find(tag.character_id).name %>
    <%= javascript_tag do %>
      clickEvent.addFrame(<%= tag.x %>, <%= tag.y %>);
      clickEvent.addNameToFrame('<%= name %>');
    <% end %>
  <% end %>
</div>